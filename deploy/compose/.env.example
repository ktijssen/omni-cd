# ============================================================
# .env.example
# ============================================================
#
# # Omni connection
# OMNI_ENDPOINT=https://your-omni-instance.example.com
# OMNI_SERVICE_ACCOUNT_KEY=base64-encoded-service-account-key
#
# # Git repo
# GIT_REPO=https://github.com/your-org/omni-gitops.git
# GIT_BRANCH=main
# GIT_TOKEN=
#
# # Sync settings
# REFRESH_INTERVAL=300
# SYNC_INTERVAL=3600
#
# # Resource paths
# MC_PATH=machine-classes
# CLUSTERS_PATH=clusters
#
# # Feature toggles
# CLUSTERS_ENABLED=true
#
# # Web UI
# WEB_PORT=8080
#
# # Logging
# LOG_LEVEL=INFO